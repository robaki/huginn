%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%%%% application of interventions to all models %%%%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

removed(ActivityOrCondition, Model) :-
	remove(ActivityOrCondition),
	model(Model).

added_to_model(ActivityOrCondition, Model) :-
	add(ActivityOrCondition),
	model(Model).
