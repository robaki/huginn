cost(1, 301) :- design_entity_loc(ent_20).
cost(1, 302) :- design_available(ent_74).
cost(1, 303) :- design_available(ent_82).
cost(5, 304) :- design_activity_rec(act_23).
cost(1, 305) :- design_entity_loc(ent_55).
