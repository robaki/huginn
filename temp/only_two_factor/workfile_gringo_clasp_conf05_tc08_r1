cost(1, 267) :- design_compartment(c_04).
cost(1, 268) :- design_entity_det(ent_61).
cost(1, 269) :- remove(setup_present(ent_65, none, c_18)).
cost(1, 270) :- add(act_62).
cost(1, 271) :- remove(setup_present(ent_51, none, c_18)).
cost(1, 272) :- design_entity_det(ent_63).
cost(1, 273) :- design_entity_loc(ent_5).
cost(1, 274) :- add(setup_present(ent_23, none, c_01)).
cost(1, 275) :- design_available(ent_34).
cost(1, 276) :- design_available(ent_29).
cost(1, 277) :- design_available(ent_76).
cost(1, 278) :- design_entity_det(ent_71).
cost(1, 279) :- remove(setup_present(ent_28, none, c_18)).
cost(1, 280) :- add(setup_present(ent_45, none, c_01)).
cost(1, 281) :- design_available(ent_72).
cost(1, 282) :- design_entity_det(ent_73).
cost(1, 283) :- design_entity_det(ent_78).
cost(1, 284) :- design_available(ent_3).
cost(1, 285) :- add(act_80).
cost(1, 286) :- add(act_61).
cost(1, 287) :- add(setup_present(ent_17, none, c_01)).
cost(1, 288) :- add(setup_present(ent_72, none, c_01)).
cost(5, 289) :- design_activity_rec(act_13).
cost(1, 290) :- design_entity_loc(ent_4).
cost(4, 291) :- design_activity_rec(act_57).
cost(1, 292) :- remove(setup_present(ent_38, none, c_18)).
cost(1, 293) :- design_entity_loc(ent_61).
cost(1, 294) :- design_available(ent_13).
cost(1, 295) :- add(act_67).
cost(1, 296) :- design_entity_loc(ent_62).
cost(1, 297) :- design_entity_det(ent_35).
cost(1, 298) :- remove(setup_present(ent_37, none, c_01)).
cost(1, 299) :- design_entity_det(ent_26).
